<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Leaftaps Automation">	
		
	<groups>
		<run>
			<exclude name = "smoke"></exclude>
			<include name = "regression"></include>
		</run>		
	</groups>
	
		<parameter name="url" value="http://leaftaps.com/opentaps/"></parameter>	
	
  <test name="Lead TestCases">
  	<parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="assignments.week7.day1.CreateLead"/>
      <class name="assignments.week7.day1.DeleteLead"/>
      <class name="assignments.week7.day1.DuplicateLead"/>
      <class name="assignments.week7.day1.EditLead"/>
    </classes>
  </test> <!-- Lead TestCases -->
  
</suite> <!-- Leaftaps Automation -->
