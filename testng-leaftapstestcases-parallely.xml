<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="LeafTaps Automation">	
	<parameter name="url" value="http://leaftaps.com/opentaps/"/>
	
	<test parallel="tests" name="LFT-96329 Lead Development in Chrome">
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="assignments.week6.leaftapstestcases.CreateLead"/>
			<class name="assignments.week6.leaftapstestcases.DeleteLead"/>
			<class name="assignments.week6.leaftapstestcases.EditLead"/>
		</classes>
	</test>
	
	<test parallel="tests" name="LFT-96330 Lead Development in Edge">
		<parameter name="browser" value="edge"/>
		<classes>
			<class name="assignments.week6.leaftapstestcases.DuplicateLead"/>
			<class name="assignments.week6.leaftapstestcases.MergeLead"/>
		</classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
